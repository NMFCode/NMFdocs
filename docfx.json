{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Expressions/**.csproj"
          ],
		  "exclude": [
			"src/Expressions/Tests/**.csproj"
		  ]
        }
      ],
      "dest": "api/Expressions",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
	{
      "src": [
        {
          "files": [
            "src/Transformations/**.csproj"
          ],
		  "exclude": [
			"src/Transformations/Tests/**.csproj"
		  ]
        }
      ],
      "dest": "api/Transformations",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
	{
      "src": [
        {
          "files": [
            "src/Synchronizations/**.csproj"
          ],
		  "exclude": [
			"src/Synchronizations/Tests/**.csproj"
		  ]
        }
      ],
      "dest": "api/Synchronizations",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "collections/**.md",
          "collections/**/toc.yml",
		  "expressions/**.md",
		  "expressions/**/toc.yml",
		  "interop/**.md",
		  "interop/**/toc.yml",
		  "models/**.md",
		  "models/**/toc.yml",
		  "optimizations/**.md",
		  "optimizations/**/toc.yml",
		  "synchronizations/**.md",
		  "synchronizations/**/toc.yml",
		  "transformations/**.md",
		  "transformations/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/nmf"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}