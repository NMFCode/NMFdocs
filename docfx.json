{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Collections/**.csproj"
          ],
		      "exclude": [
			      "src/Collections/*Test/**.csproj"
		      ]
        }
      ],
      "dest": "collections/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "src/Expressions/**.csproj"
          ],
		      "exclude": [
			      "src/Expressions/Tests/**.csproj"
		      ]
        }
      ],
      "dest": "expressions/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },    
    {
      "src": [
        {
          "files": [
            "src/Interop/**.csproj"
          ],
		      "exclude": [
			      "src/Interop/*Tests/**.csproj"
		      ]
        }
      ],
      "dest": "interop/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },   
    {
      "src": [
        {
          "files": [
            "src/Models/**.csproj"
          ],
		      "exclude": [
			      "src/Models/*Tests/**.csproj"
		      ]
        }
      ],
      "dest": "models/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },   
    {
      "src": [
        {
          "files": [
            "src/Optimizations/**.csproj"
          ],
		      "exclude": [
			      "src/Optimizations/*Tests/**.csproj"
		      ]
        }
      ],
      "dest": "optimizations/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },
	  {
      "src": [
        {
          "files": [
            "src/Synchronizations/**.csproj"
          ],
		      "exclude": [
			      "src/Synchronizations/Synchronizations.Tests/**.csproj",
			      "src/Synchronizations/Synchronizations.Models.Tests/**.csproj"
		      ]
        }
      ],
      "dest": "synchronizations/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    },
	  {
      "src": [
        {
          "files": [
            "src/Transformations/**.csproj"
          ],
		      "exclude": [
			      "src/Transformations/Tests/**.csproj",
			      "src/Transformations/Transformations.Tests/**.csproj"
		      ]
        }
      ],
      "dest": "transformations/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
          "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "collections/**.md",
          "collections/**.yml",
		      "expressions/**.md",
		      "expressions/**.yml",
		      "interop/**.md",
		      "interop/**.yml",
		      "models/**.md",
		      "models/**.yml",
		      "optimizations/**.md",
		      "optimizations/**.yml",
		      "synchronizations/**.md",
		      "synchronizations/**.yml",
		      "transformations/**.md",
		      "transformations/**.yml",
		      "publications/**.md",
		      "publications/**.yml",
          "setup/**.md",
          "setup/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "README.md",
          "CONTRIBUTING.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "collections/**.PNG",
          "collections/**.png",
          "collections/**.zip",
		      "expressions/**.PNG",
		      "expressions/**.png",
		      "expressions/**.zip",
		      "interop/**.PNG",
		      "interop/**.png",
		      "interop/**.zip",
		      "models/**.PNG",
		      "models/**.png",
		      "models/**.zip",
		      "optimizations/**.PNG",
		      "optimizations/**.png",
		      "optimizations/**.zip",
		      "synchronizations/**.PNG",
		      "synchronizations/**.png",
		      "synchronizations/**.zip",
		      "transformations/**.PNG",
		      "transformations/**.png",
		      "transformations/**.zip",
          "setup/images/**.png"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**/api/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/nmf"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}